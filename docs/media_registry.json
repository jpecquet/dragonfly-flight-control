  "version": 1,
  "entries": [
    {
      "id": "azuma1985_translate_sim",
      "description": "Regenerate Azuma 1985 translated config and simulation data.",
      "env": {
        "MPLCONFIGDIR": "runs/azuma1985/demo/.cache/matplotlib",
        "XDG_CACHE_HOME": "runs/azuma1985/demo/.cache"
      },
      "commands": [
        [
          "python",
          "scripts/azuma1985_pipeline.py",
          "translate",
          "--run-dir",
          "runs/azuma1985/demo"
        ],
        [
          "python",
          "scripts/azuma1985_pipeline.py",
          "sim",
          "--run-dir",
          "runs/azuma1985/demo"
        ]
      ],
      "outputs": [
        "runs/azuma1985/demo/manifest.json",
        "runs/azuma1985/demo/sim/sim_azuma1985.cfg",
        "runs/azuma1985/demo/sim/translate_summary.json",
        "runs/azuma1985/demo/sim/output.h5"
      ],
      "sync": [
        {
          "from": "runs/azuma1985/demo/manifest.json",
          "to": "docs/case_studies/azuma1985/artifacts/manifest.json"
        },
        {
          "from": "runs/azuma1985/demo/sim/sim_azuma1985.cfg",
          "to": "docs/case_studies/azuma1985/artifacts/sim/sim_azuma1985.cfg"
        },
        {
          "from": "runs/azuma1985/demo/sim/translate_summary.json",
          "to": "docs/case_studies/azuma1985/artifacts/sim/translate_summary.json"
        },
        {
          "from": "runs/azuma1985/demo/sim/output.h5",
          "to": "docs/case_studies/azuma1985/artifacts/sim/output.h5"
        }
      ]
    },
    {
      "id": "azuma1985_animation_light",
      "description": "Regenerate Azuma 1985 Blender animation for docs (light theme).",
      "env": {
        "MPLCONFIGDIR": "runs/azuma1985/demo/.cache/matplotlib",
        "XDG_CACHE_HOME": "runs/azuma1985/demo/.cache"
      },
      "commands": [
        [
          "python",
          "-m",
          "post.plot_simulation",
          "runs/azuma1985/demo/sim/output.h5",
          "docs/_static/media/azuma1985/simulation.light.mp4",
          "--config",
          "docs/case_studies/azuma1985/render_config.dark.json",
          "--theme",
          "light",
          "--frame-step",
          "3"
        ]
      ],
      "outputs": [
        "docs/_static/media/azuma1985/simulation.light.mp4"
      ],
      "sync": []
    },
